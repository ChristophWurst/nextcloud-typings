{
  "name": "@nextcloud/typings",
  "version": "2.0.0",
  "description": "Nextcloud TypeScript typings",
  "keywords": [
    "nextcloud",
    "typescript"
  ],
  "homepage": "https://github.com/nextcloud-libraries/nextcloud-typings#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/nextcloud-libraries/nextcloud-typings"
  },
  "license": "GPL-3.0-or-later",
  "author": "Nextcloud GmbH and Nextcloud contributors",
  "type": "module",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts"
    },
    "./ocs": {
      "types": "./lib/ocs.d.ts"
    },
    "./Nextcloud*": {
      "types": "./lib/Nextcloud*/index.d.ts"
    }
  },
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "AUTHORS.md",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "format": "prettier -c .",
    "format:fix": "prettier -w .",
    "lint": "eslint lib",
    "lint:fix": "eslint --fix lib",
    "test": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": "@nextcloud/eslint-config/typescript"
  },
  "prettier": "@nextcloud/prettier-config",
  "dependencies": {
    "@types/jquery": "^3.5.30"
  },
  "devDependencies": {
    "@nextcloud/eslint-config": "^8.4.1",
    "@nextcloud/prettier-config": "^1.1.0",
    "eslint": "^8.57.0",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": "^20.0.0",
    "npm": "^10.0.0"
  }
}
